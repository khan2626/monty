#include "monty.h"

/**
 * addqueue - add node to rear of stack
 * @n: new value
 * @top: pointer to the first element of stack
 * Return: Nothing
 */

void addqueue(stack_t **top, int n)
{
  stack_t *new_node, *temp;

  temp = *top;
  new_node = malloc(sizeof(struct_t));
  if (new_node == NULL)
    {
      printf("Error\n");
    }
  new_node->n = n;
  new_node->next = NULL;
  if (temp)
    {

      while (temp->next)
	temp = temp->next;
    }
  if (!temp)
    {

      *top = new_node;
      new_node->prev = NULL;
    }
  else
    {
      temp->next = new_node;
      new_node->prev = temp;
    }
}
